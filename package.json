  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/eslint-plugin": "^11.12.0",
    "@emotion/react": "^11.14.0",
    "@types/react": "^18.2.0",
    "@types/solr-client": "^0.7.9",
    "@typescript-eslint/eslint-plugin": "5.62.0",
