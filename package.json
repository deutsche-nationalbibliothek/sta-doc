{
  "name": "sta-doc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "node --max-old-space-size=9192 node_modules/next/dist/bin/next build",
    "start": "next start",
    "data": "ts-node bin/data",
    "data:debug": "NODE_OPTIONS='--inspect' ts-node bin/data",
    "data:fetch": "ts-node bin/data fetch",
    "data:fetch:single": "ts-node bin/data fetch:single",
    "data:fetch:properties-items": "ts-node bin/data fetch:properties-items",
    "data:parse": "ts-node bin/data parse",
    "data:parse:fr": "ts-node bin/data parse:fr",
    "data:parse:single": "ts-node bin/data parse:single",
    "data:parse:single:fr": "ts-node bin/data parse:single:fr",
    "data:parse:codings": "ts-node bin/data parse:codings",
    "data:parse:labels": "ts-node bin/data parse:labels",
    "data:parse:propertyTypes": "ts-node bin/data parse:propertyTypes",
    "data:parse:staNotations": "ts-node bin/data parse:staNotations",
    "data:parse:staNotationsFr": "ts-node bin/data parse:staNotationsFr",
    "split-entities": "./bin/split-entities-solr.sh",
    "split-entities-powershell": "powershell -File ./bin/split-entities-solr.ps1",
    "docker:up": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "docker:build": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml build",
    "docker:solr:index": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml exec solr sh index.sh",
    "docker:dev:up": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d",
    "docker:dev:build": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml build",
    "docker:dev:solr:index": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml exec solr sh index.sh",
    "lint": "next lint",
    "test": "NODE_OPTIONS='--max-old-space-size=6144' jest --watch",
    "test:debug": "NODE_OPTIONS='--max-old-space-size=6144 --inspect' jest --watch",
    "test:ci": "jest --ci"
  },
